$(document).ready(function() {
    // Initialize DataTables
    var table = $('#example').DataTable({
        dom: 'Bfrtip',
        buttons: [
            {
                extend: 'excelHtml5',
                text: 'Export to Excel',
                exportOptions: {
                    columns: ':visible', // Export only visible columns
                    format: {
                        body: function (data, row, column, node) {
                            // Check if the column is the E column
                            if (column === 4) { // Assuming E column is index 4
                                return { t: 's', v: data }; // Set data type to string for E column
                            }
                            return data;
                        }
                    }
                }
            }
        ]
    });
});
