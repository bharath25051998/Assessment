using System;

class Program
{
    static void Main()
    {
        // Example usage:
        string startMonth = "May";
        int result = CountMonthsFromQuarters(startMonth);

        Console.WriteLine($"Count of months: {result}");
    }

    static int CountMonthsFromQuarters(string startMonth)
    {
        string[] months = { "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" };

        int startIndex = Array.IndexOf(months, startMonth);
        int monthsInQuarter = 3;
        int endIndex = startIndex + monthsInQuarter * 3; // Counting the current quarter and the next two quarters

        // Handling wrap-around if endIndex goes beyond December
        int count = Math.Min(12 - startIndex, monthsInQuarter * 3) + endIndex % 12;
        
        return count;
    }
}
