function postToOtherAction(param1, param2) {
    // Create a form element
    var form = $('<form>', {
        'method': 'POST',
        'action': '/ControllerName/ActionName'
    });

    // Add parameters as hidden fields
    $('<input>').attr({
        'type': 'hidden',
        'name': 'param1',
        'value': param1
    }).appendTo(form);
    
    $('<input>').attr({
        'type': 'hidden',
        'name': 'param2',
        'value': param2
    }).appendTo(form);

    // Append the form to the body and submit it
    form.appendTo('body').submit();
}
