using System.Linq;

// Assuming your data is stored in a List<Data> where Data is the class representing your data
// Replace Data with your actual class name

// Define your groups
var groups = yourDataList.GroupBy(d =>
{
    if (d.DDStatusID < 7)
        return "Group1";
    else if (d.DDStatusID >= 7 && d.DDStatusID <= 14)
        return "Group2";
    else if (d.DDStatusID >= 14 && d.DDStatusID <= 20)
        return "Group3";
    else if (d.DDStatusID > 20 && d.DDStatusID <= 30)
        return "Group4";
    else if (d.DDStatusID == 31 || d.DDStatusID == 32 || d.DDStatusID == 33)
        return "Group5";
    else
        return "Other";
});

// Create a ViewBag to hold the grouped data
ViewBag.GroupedData = groups.ToDictionary(g => g.Key, g => g.ToList());
