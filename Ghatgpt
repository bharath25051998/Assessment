<!-- Table to display previously saved data -->
<table>
  <thead>
    <tr>
      @foreach (var savedData in ViewBag.SavedDataDictionary)
      {
          if (!string.IsNullOrEmpty(savedData.Value))
          {
              <th>@ViewBag.KickerData[savedData.Key].KickerName</th>
          }
      }
      <th>Additional Header</th> <!-- New additional header -->
    </tr>
  </thead>
  <tbody>
    <tr>
      @foreach (var savedData in ViewBag.SavedDataDictionary)
      {
          if (!string.IsNullOrEmpty(savedData.Value))
          {
              <td>@string.Format("{0:C}", Convert.ToDecimal(savedData.Value))</td>
          }
      }
      <td>Additional Value</td> <!-- New additional value -->
    </tr>
  </tbody>
</table>
