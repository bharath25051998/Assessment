for (i = 0; i < spocRating.length; i++) {
  var value = $("#" + spocRating[i].id).val();
  // This regex checks if the value has multiple zeros
  if (value === "" || /^0{2,}$/.test(value)) {
    isValid = false;
    errorMsg = "Please enter a valid rating. Multiple zeros are not allowed.";
    break;
  }
}
