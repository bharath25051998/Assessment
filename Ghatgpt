SELECT chapter, COUNT(*) AS chapter_count
FROM (
    SELECT DISTINCT chapter, from_date, to_date
    FROM your_table
    WHERE 
        MONTH(from_date) = MONTH(CURRENT_DATE()) AND YEAR(from_date) = YEAR(CURRENT_DATE())
        AND 
        MONTH(to_date) = MONTH(CURRENT_DATE()) AND YEAR(to_date) = YEAR(CURRENT_DATE())
) AS subquery
GROUP BY chapter;
