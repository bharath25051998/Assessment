var ws1 = XLSX.utils.table_to_sheet($('#table1')[0]);
treatColumnsAsText(ws1, [1]); // Treat column 1 as text
XLSX.utils.book_append_sheet(wb, ws1, 'Table 



// Treat specific columns as text (string)
function treatColumnsAsText(ws, columns) {
    var range = XLSX.utils.decode_range(ws['!ref']);
    
    columns.forEach(function(column) {
        for (var R = range.s.r; R <= range.e.r; ++R) {
            var cellRef = XLSX.utils.encode_cell({ r: R, c: column });
            if (ws[cellRef] && typeof ws[cellRef].v === 'string' && ws[cellRef].v.endsWith('%')) {
                ws[cellRef].t = 's'; // Treat as string
            }
        }
    });
}
