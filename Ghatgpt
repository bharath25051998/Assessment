@{
    var averages = ViewBag.Averages as Dictionary<string, double>;
}

@foreach (var item in averages)
{
    <div id="@("average_" + item.Key)">
        Key: @item.Key, Average: @item.Value
    </div>
}

<!-- Or if you want to target a specific HTML element by ID -->
<script type="text/javascript">
    window.onload = function() {
        @foreach (var item in averages)
        {
            @:document.getElementById("average_@item.Key").innerText = "Average for @item.Key: @item.Value";
        }
    }
</script>
