DECLARE @CurrentQuarterStart DATE;
DECLARE @NextTwoQuartersEnd DATE;

SET @CurrentQuarterStart = DATEADD(QUARTER, DATEDIFF(QUARTER, 0, GETDATE()), 0);
SET @NextTwoQuartersEnd = DATEADD(QUARTER, 3 * 2, @CurrentQuarterStart);

SELECT @CurrentQuarterStart AS CurrentQuarterStart, @NextTwoQuartersEnd AS NextTwoQuartersEnd;
