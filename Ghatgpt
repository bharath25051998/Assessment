SELECT 
    CONCAT(DATENAME(MONTH, FromDate), '-', DATEPART(YEAR, FromDate)) AS Month_Year,
    COUNT(DISTINCT CASE WHEN LocationIndex = 'onsite' THEN EmployeeEmail END) AS Distinct_Onsite_Employee_Count
FROM 
    YourTempTable
GROUP BY 
    CONCAT(DATENAME(MONTH, FromDate), '-', DATEPART(YEAR, FromDate))
