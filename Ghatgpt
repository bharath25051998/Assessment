using System;
using System.Data;
using System.Collections.Generic;
using System.Linq;

class Program
{
    static void Main()
    {
        // Sample DataTables
        DataTable dt1 = new DataTable();
        dt1.Columns.Add("Column1");
        dt1.Columns.Add("Column2");
        dt1.Rows.Add("Value1_1", DBNull.Value);
        dt1.Rows.Add("Value2_1", "Value2_2");

        DataTable dt2 = new DataTable();
        dt2.Columns.Add("Column1");
        dt2.Columns.Add("Column2");
        dt2.Rows.Add("Value3_1", "Value3_2");
        dt2.Rows.Add(DBNull.Value, "Value4_2");

        // List to hold individual values
        List<string> combinedList = new List<string>();

        // Process dt1 rows
        foreach (DataRow row in dt1.Rows)
        {
            foreach (var item in row.ItemArray)
            {
                if (item != DBNull.Value)  // Exclude null values
                {
                    combinedList.Add(item.ToString());
                }
            }
        }

        // Process dt2 rows
        foreach (DataRow row in dt2.Rows)
        {
            foreach (var item in row.ItemArray)
            {
                if (item != DBNull.Value)  // Exclude null values
                {
                    combinedList.Add(item.ToString());
                }
            }
        }

        // Print the result
        foreach (var value in combinedList)
        {
            Console.WriteLine(value);
        }
    }
}
